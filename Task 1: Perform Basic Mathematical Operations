num1 = int(input("Enter the first number: ")) #Takes the first number
num2 = int(input("Enter the second number: "))  #Takes the second number
print()  #Prints an empty line
print("Addition: ", num1+num2,"\nSubstraction: ", num1-num2, "\nMultiplication: ", num1*num2, "\nDivision: ", num1/num2)  #Prints the operations
